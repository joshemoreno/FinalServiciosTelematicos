curl -i http://localhost:5000/books
curl -i http://localhost:5000/books/2
curl -i -H "Content-Type: application/json" -X POST -d '{"title":"El nuevo libro"}' http://localhost:5000/books
curl -i -H "Content-Type: application/json" -X PUT -d '{"author":"Jose Antonio"}' http://localhost:5000/books/2
curl -i -H "Content-Type: application/json" -X DELETE http://localhost:5000/books/1
